{
  "trailingSlash": false,
  "rewrites": [
    { "source": "/pages/:name", "destination": "/pages/:name.html" }
  ],
  "headers": [
    { "source": "/(css|js|assets)/(.*)", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
    { "source": "/service-worker.js", "headers": [{ "key": "Cache-Control", "value": "no-store" }] }
  ]
}